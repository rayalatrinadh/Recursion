package com.Recursion;

public class FactorialNumProperWay {
    public static void main(String[] args) {
        int ans = factorialNum(5);
        System.out.println(ans);
    }
    static int factorialNum(int n){
  
        if(n == 1){
            return n;
        }

        return n * factorialNum(n-1);
    }
}
